function [stretchVector] = stretchVec(ogVector, scaleValue)
stretchVector = ogVector;

stretchIdx = 1;

for idx = 1:length(ogVector)
    rCount = 0;
    
    while (rCount < scaleValue)
       stretchVector = stretchVector(1:stretchIdx)
       
       rCount = rCount + 1; 
       stretchIdx = stretchIdx + 1;
    end
end
end

